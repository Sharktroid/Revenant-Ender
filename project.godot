; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Revenant Ender"
config/tags=PackedStringArray("2d")
run/main_scene="res://renderer/renderer.tscn"
config/features=PackedStringArray("4.4")

[autoload]

Utilities="*res://utilities/utilities.gd"
HelpPopupController="*res://ui/help_popup/help_popup_controller.gd"
MapController="*res://controllers/map_controller/map_controller.gd"
GameController="*res://controllers/game_controller/game_controller.gd"
AudioPlayer="*res://audio/audio_player.gd"
CursorController="*res://cursor/cursor_controller.gd"
AttackController="*res://controllers/attack controller/attack_controller.gd"
Options="*res://controllers/game_controller/config/options.gd"

[debug]

gdscript/warnings/untyped_declaration=1
gdscript/warnings/unsafe_property_access=1
gdscript/warnings/unsafe_method_access=1
gdscript/warnings/unsafe_call_argument=1
gdscript/completion/autocomplete_setters_and_getters=true

[display]

window/size/viewport_width=512
window/size/viewport_height=288
window/stretch/mode="canvas_items"
window/vsync/vsync_mode=3

[global]

warning=false
frame=false

[gui]

theme/custom="res://ui/theme/menu_theme.tres"

[importer_defaults]

